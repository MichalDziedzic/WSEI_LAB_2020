ne":232,"column":0}}],"sources":{"../CSS/style.css":"@import url(\"https://fonts.googleapis.com/css2?family=Lato:wght@400;700;900&display=swap\");\r\n\r\nbody {\r\n  margin: 0 auto;\r\n  padding: 0 0 0 0;\r\n  font-family: \"Lato\", sans-serif;\r\n  box-sizing: border-box;\r\n}\r\n#wrapper {\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\nheader {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  background: black;\r\n  color: white;\r\n  font-size: 40px;\r\n  min-height: 10vh;\r\n  max-width: 100%;\r\n}\r\nheader > h1 {\r\n  padding: 15px 10px 15px 20px;\r\n}\r\nsection {\r\n  width: 100%;\r\n}\r\nnav {\r\n  max-width: 20%;\r\n  width: 15%;\r\n  font-size: 15px;\r\n  min-height: 10vh;\r\n}\r\n.dropdown {\r\n  height: 10vh;\r\n  display: flex;\r\n  max-width: 100%;\r\n  justify-content: space-around;\r\n  align-items: center;\r\n  min-height: 100%;\r\n}\r\n\r\n.projects,\r\n.products {\r\n  position: relative;\r\n}\r\n\r\n.projects ul,\r\n.products ul {\r\n  margin-top: 45px;\r\n  position: absolute;\r\n  display: flex;\r\n  justify-content: space-around;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  width: 300px;\r\n  height: 200px;\r\n  background: white;\r\n  right: 0;\r\n  list-style: none;\r\n  border-radius: 5px;\r\n  opacity: 0;\r\n  pointer-events: none;\r\n  transform: translateY(-25px);\r\n  transition: all 0.4s ease;\r\n}\r\n.projects li,\r\n.products li {\r\n  width: 100%;\r\n  height: 100%;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  background: black;\r\n}\r\n\r\n.projects li:hover,\r\n.products li:hover {\r\n  background-color: rgb(197, 173, 181);\r\n  cursor: pointer;\r\n}\r\n\r\n.projects a,\r\n.products a {\r\n  color: white;\r\n  text-decoration: none;\r\n}\r\n\r\n.vinHistory-test {\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n  width: 100%;\r\n  padding: 0 15px 0 15px;\r\n}\r\n\r\n.dropdown button,\r\n.home {\r\n  background: none;\r\n  text-decoration: none;\r\n  border: none;\r\n  color: white;\r\n  font-size: 25px;\r\n  cursor: pointer;\r\n}\r\n\r\n.projects button:focus + ul,\r\n.products button:focus + ul {\r\n  opacity: 1;\r\n  pointer-events: all;\r\n  transform: translateY(-10px);\r\n}\r\n\r\n/* --------------------- */\r\n.firstPage {\r\n  width: 100%;\r\n  height: 100vh;\r\n  min-height: 100vh;\r\n}\r\n\r\n.vinDecoder {\r\n  width: 100%;\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n.vinHeader,\r\n.vinFinder {\r\n  width: 100%;\r\n  display: flex;\r\n  justify-content: center;\r\n}\r\n.vinFinder {\r\n  /* margin: 10px 0 20px 0; */\r\n  min-height: 20vh;\r\n  align-items: center;\r\n}\r\n#InputVin {\r\n  height: 25px;\r\n}\r\n.btn,\r\n.detailsBtn {\r\n  height: 31px;\r\n  background: white;\r\n  border: 1px solid black;\r\n  border-radius: 2px;\r\n}\r\n.vinHeader {\r\n  font-size: 38px;\r\n\r\n  min-height: 15vh;\r\n  align-items: flex-end;\r\n}\r\n.vinArticle {\r\n  display: flex;\r\n  flex-direction: column;\r\n\r\n  min-height: 55vh;\r\n  padding: 50px;\r\n}\r\n\r\n.vinArticle > p {\r\n  width: 100%;\r\n  line-height: 1.3;\r\n  padding: 30px 0 30px 0;\r\n}\r\n\r\n.vinArticle > h3 {\r\n  font-size: 25px;\r\n}\r\nmain,\r\naside {\r\n  width: 100%;\r\n  min-height: 100vh;\r\n}\r\n\r\nmain {\r\n  background: palevioletred;\r\n}\r\n.headerRaport {\r\n  width: 100%;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  min-height: 20vh;\r\n  background: yellow;\r\n}\r\n.headerRaport > h3 {\r\n  font-size: 38px;\r\n}\r\n.infoRaport,\r\n.imgRaport {\r\n  width: 50%;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  font-size: 16px;\r\n}\r\n.mainRaport {\r\n  display: flex;\r\n  width: 100%;\r\n  min-height: 60vh;\r\n}\r\n.detailsRaport {\r\n  min-height: 20vh;\r\n  background: rebeccapurple;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n.imgRaport {\r\n  background: white;\r\n}\r\n.imgRaport > img {\r\n  object-fit: cover;\r\n  height: 55vh;\r\n  width: 90%;\r\n}\r\n.ContainerDetailsRaport {\r\n  min-height: 40vh;\r\n  width: 100%;\r\n  background: orange;\r\n  display: none;\r\n}\r\naside {\r\n  background: seagreen;\r\n}\r\nfooter {\r\n  background-color: gray;\r\n  color: azure;\r\n  min-height: 100px;\r\n}\r\n"},"lineCount":232}},"error":null,"hash":"e26f1a92175125d992f6604273984113","cacheData":{"env":{}}}